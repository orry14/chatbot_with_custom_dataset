function APICall(){function n(){}this.PopulateJQGrid=function(n,t,i){i.clearGridData();$.support.cors=!0;var r="",f=this.ServiceName,u=JSON.stringify(n);return $.ajax({async:!1,type:"POST",url:_url,data:u,contentType:"application/json; charset=utf-8",dataType:"json",processdata:!0,success:function(n){var u=n,t;if(u=="[]")WriteLog("No Records Found");else if(u==null)WriteLog("Unable to connect to server");else{try{r=jQuery.parseJSON(u)}catch(f){r=null}if(r!=null)for(t=0;t<r.length;t++)i.addRowData(t+1,r[t]);else WriteLog(u.toString())}},complete:function(){$.hideprogress()},error:function(n,t,i){r=null;i.toString()=="Unknown"?WriteLog("Unable to connect to server"):WriteLog(i.toString())}}),r};this.InsertDataFixString=function(strJsonDatas,strFunction,strFixstring){$.support.cors=!0;var Result="",ServiceName=this.ServiceName,_url=strFunction,dt=JSON.stringify(strJsonDatas),msg2=eval({strJsonData:dt,strSessionID:null,strFixedString:strFixstring});return msg2=JSON.stringify(msg2),$.ajax({type:"POST",url:_url,data:msg2,contentType:"application/json; charset=utf-8",async:!1,dataType:"json",processdata:!0,success:function(n){var t=n;t=="[]"?(Result="Error",WriteLog(Result)):t==null?(Result="Unable to connect to server",WriteLog(Result)):Result=t=="Success"?t:t},error:function(n,t,i){Result=null;Result=i.toString()=="Unknown"?"Unable to connect to server":"error.toString()";WriteLog(Result)},complete:function(){}}),Result};this.PopulateJQGridUsingURL=function(n,t,i){i.clearGridData();$.support.cors=!0;var r="",u=JSON.stringify(n);return $.ajax({async:!1,type:"POST",url:t,data:u,contentType:"application/json; charset=utf-8",dataType:"json",processdata:!0,success:function(n){var u=n,t;if(u=="[]")WriteLog("No Records Found");else if(u==null)WriteLog("Unable to connect to server");else{try{r=jQuery.parseJSON(u)}catch(f){r=null}if(r!=null)for(t=0;t<r.length;t++)i.addRowData(t+1,r[t]);else WriteLog(u.toString())}},error:function(n,t,i){r=null;i.toString()=="Unknown"?WriteLog("Unable to connect to server"):WriteLog(i.toString())}}),r};this.SelectData=function(n,t){$.support.cors=!0;var i="",f=this.ServiceName,r=t,u=JSON.stringify(n);return $.ajax({async:!1,type:"POST",url:r,data:u,contentType:"application/json; charset=utf-8",dataType:"json",processdata:!0,success:function(n){n=JSON.stringify(n);var t=n;if(t=="[]")WriteLog("No Records Found");else if(t==null)WriteLog("Unable to connect to server");else{try{i=jQuery.parseJSON(t)}catch(r){i=null}i==null&&WriteLog(t.toString())}},error:function(n,t,r){i=null;r.toString()=="Unknown"?WriteLog("Unable to connect to server"):WriteLog(r.toString())}}),i};this.DeleteData=function(t,i){$.support.cors=!0;var r="",u=i,f=JSON.stringify(t);return $.ajax({async:!1,type:"POST",url:u,data:f,contentType:"application/json; charset=utf-8",dataType:"json",processdata:!0,success:function(n){var t=n;t=="[]"?(r="Error",WriteLog(r)):t==null&&(r="Unable to connect to server",WriteLog(r));(t="Success")?r=t:(r="Error",WriteLog(r))},error:function(n){r=null;r=n.responseText},complete:function(t,i){t.readyState===4&&t.status===200&&n(t,i)}}),r};this.DeletePhoto=function(t,i){$.support.cors=!0;var r="",u=i,f=JSON.stringify(t);return $.ajax({async:!1,type:"POST",url:u,data:f,contentType:"application/json; charset=utf-8",dataType:"json",processdata:!0,success:function(n){var t=n;t=="[]"?(r="Error",WriteLog(r)):t==null&&(r="Unable to connect to server",WriteLog(r));(t="Success")?r=t:(r="Error",WriteLog(r))},error:function(n){r=null;r=n.responseText},complete:function(t,i){t.readyState===4&&t.status===200&&n(t,i)}}),r};this.UpdateData=function(strJsonDatas,strFunction,Condition){$.support.cors=!0;var Result="",_url=strFunction,dt=JSON.stringify(strJsonDatas),msg2=eval({strSessionID:UserSession,strJsonData:dt,strCondition:Condition});return msg2=JSON.stringify(msg2),$.ajax({async:!1,type:"POST",url:_url,data:msg2,async:!1,contentType:"application/json; charset=utf-8",dataType:"text",processdata:!0,success:function(n){var t=n;return t=="[]"?(Result="Error",WriteLog(Result)):t==null&&(Result="Unable to connect to server",WriteLog(Result)),(t="Success")?Result=t:(Result="Error",WriteLog(Result)),Result},error:function(n,t,i){Result=null;Result=i.toString()=="Unknown"?"Unable to connect to server":"error.toString()";WriteLog(Result)},complete:function(t,i){t.readyState===4&&t.status===200&&n(t,i)}}),Result};this.Sendmail=function(strJsonDatas,strFunction){$.support.cors=!0;var Result=null,_url=strFunction,dt=JSON.stringify(strJsonDatas),msg2=eval({strJsonData:dt,strSessionID:UserSession});return msg2=JSON.stringify(msg2),$.ajax({type:"POST",url:_url,data:msg2,contentType:"application/json; charset=utf-8",async:!1,dataType:"text",processdata:!0,success:function(n){Result=n},error:function(n){Result=n.responseText}}),Result};this.InsertData=function(strJsonDatas,strFunction){$.support.cors=!0;var Result="",ServiceName=this.ServiceName,_url=strFunction,dt=JSON.stringify(strJsonDatas),msg2=eval({strJsonData:dt,strSessionID:null});return msg2=JSON.stringify(msg2),$.ajax({type:"POST",url:_url,data:msg2,contentType:"application/json; charset=utf-8",async:!1,dataType:"json",processdata:!0,success:function(n){var t=n;t=="[]"?(Result="Error",WriteLog(Result)):t==null?(Result="Unable to connect to server",WriteLog(Result)):Result=t=="Success"?t:t},error:function(n,t,i){Result=null;Result=i.toString()=="Unknown"?"Unable to connect to server":"error.toString()";WriteLog(Result)},complete:function(){}}),Result};this.InsertAuditData=function(strJsonDatas,strFunction){$.support.cors=!0;var Result="",ServiceName=this.ServiceName,_url=RestServiceURL+ServiceName+strFunction,dt=JSON.stringify(strJsonDatas),msg2=eval({strJsonData:dt,strSessionID:UserSession});return msg2=JSON.stringify(msg2),$.ajax({type:"POST",url:_url,data:msg2,contentType:"application/json; charset=utf-8",async:!1,dataType:"json",processdata:!0,success:function(n){var t=n;if(t=="[]")Result="Error",WriteLog(Result);else if(t==null)Result="Unable to connect to server",WriteLog(Result);else if(t=="Success")Result=t;else try{Result=jQuery.parseJSON(t)}catch(i){WriteLog("Parse Error");WriteLog(i);Result=t}},error:function(n,t,i){Result=null;Result=i.toString()=="Unknown"?"Unable to connect to server":"error.toString()";WriteLog(Result)},complete:function(n){if(n.readyState===4&&n.status===200)return Result}}),Result};this.GetCurrentDate=function(){var n=new Date,t=n.getFullYear(),i=n.getMonth()+1,r=n.getDate();return t+"/"+i+"/"+r};this.CallAPIServerFunction=function(n,t){var r,i;try{return r=JSON.stringify(t),$.support.cors=!0,i="",$.ajax({type:"POST",url:n,data:r,contentType:"application/json; charset=utf-8",dataType:"json",async:!1,processdata:!0,success:function(n){var t=n;if(t=="[]")i="Error",WriteLog(i);else if(t==null)i="Unable to connect to server",WriteLog(i);else if(t=="Success")i=t;else try{i=jQuery.parseJSON(t)}catch(r){WriteLog("Parse Error");WriteLog(r);i=t}},error:function(n,t,r){i=null;i=r.toString()=="Unknown"?"Unable to connect to server":"error.toString()";WriteLog(i)}}),i}catch(u){return null}}}function WriteLog(){}this.showmsg=function(n,t,i,r,u,f){var e;e='<table width="100%" cellpadding="1" cellspacing="0" border="0" class="'+i+'"><tr><td style="width:30px;" align="center" valign="middle">'+r+"<\/td><td>"+t+'<\/td><td style="width:30px;" align="center" valign="middle"><a href="javascript:void(0);" onclick="$(\'#'+n+"').toggle(400);\">"+u+"<\/a><\/td><\/tr><\/table>";$("#"+n).html(e);$("#"+n).show();f&&setTimeout(function(){$("#"+n).fadeOut("normal")},1e4)}